{
  "name": "angular-table-utils",
  "description": "Angular2 data table utils",
  "main": "./bundles/angular-table-utils.umd.js",
  "version": "0.0.20",
  "module": "./index.js",
  "typings": "./index.d.ts",
  "scripts": {
    "start": "concurrently --raw \"webpack-dev-server --open\" ",
    "build:demo": "webpack -p",
    "build:umd": "webpack --config webpack.config.umd.ts",
    "build:ngc": "ngc -p tsconfig-ngc.json",
    "build:dist": "npm run build:umd && npm run build:ngc",
    "build:clean": "del-cli dist",
    "test": "karma start --single-run",
    "test:watch": "karma start --auto-watch",
    "commit": "git add . && git-cz",
    "compodoc": "compodoc -p tsconfig-compodoc.json -d docs --disableGraph --disableCoverage --disablePrivateOrInternalSupport",
    "gh-pages": "git checkout gh-pages && git merge master --no-edit --no-ff && npm run build:demo && npm run compodoc && git add . && git commit -m \"chore: build demo and docs\" && git push && git checkout master",
    "copyfiles": "copyfiles package.json LICENSE README.md CHANGELOG.md dist",
    "release:git": "git add . && git commit -m \"chore: bump version number\" && standard-version  && git push --follow-tags origin master",
    "release:npm": "npm run build:dist && npm run copyfiles && npm publish dist",
    "release": "npm run release:git && npm run release:npm",
    "postrelease": "npm run build:clean && npm run gh-pages",
    "commitmsg": "validate-commit-msg",
    "codecov": "cat coverage/lcov.info | codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jbmarchetti/angular-table-utils.git"
  },
  "keywords": [
    "angular2",
    "angular"
  ],
  "author": "Jean Baptiste Marchetti",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jbmarchetti/angular-table-utils/issues"
  },
  "homepage": "https://github.com/jbmarchetti/angular-table-utils#readme",
  "devDependencies": {
    "@angular/common": "^2.1.0",
    "@angular/compiler": "^2.1.0",
    "@angular/compiler-cli": "^2.1.0",
    "@angular/core": "^2.1.0",
    "@angular/forms": "^2.1.0",
    "@angular/platform-browser": "^2.1.0",
    "@angular/platform-browser-dynamic": "^2.1.0",
    "@ngx-translate/core": "^6.0.1",
    "@types/chai": "^3.4.30",
    "@types/jasmine": "2.2.33",
    "@types/node": "7.0.0",
    "@types/sinon": "^1.16.27",
    "@types/sinon-chai": "^2.7.26",
    "@types/webpack": "^2.2.8",
    "awesome-typescript-loader": "^3.0.8",
    "chai": "^3.5.0",
    "codecov-lite": "^0.1.3",
    "codelyzer": "^2.0.0",
    "commitizen": "^2.8.1",
    "compodoc": "^0.0.41",
    "concurrently": "^3.0.0",
    "copyfiles": "^1.2.0",
    "core-js": "^2.4.1",
    "cz-conventional-changelog": "^2.0.0",
    "del-cli": "^0.2.1",
    "husky": "^0.13.0",
    "imports-loader": "^0.7.0",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jasmine-core": "^2.5.0",
    "karma": "^1.4.1",
    "karma-coverage-istanbul-reporter": "^0.3.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.1",
    "phantomjs-prebuilt": "^2.1.7",
    "rxjs": "^5.0.1",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0",
    "standard-version": "^4.0.0",
    "ts-node": "^2.0.0",
    "tslint": "^4.0.2",
    "tslint-loader": "^3.3.0",
    "typescript": "~2.0.3",
    "validate-commit-msg": "^2.8.0",
    "webpack": "^2.2.0",
    "webpack-angular-externals": "^1.0.0",
    "webpack-dev-server": "^2.2.0",
    "webpack-rxjs-externals": "^1.0.0",
    "zone.js": "^0.7.4"
  },
  "peerDependencies": {
    "@angular/core": "^2.0.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "angular-forms-utils": "^0.2.2",
    "angular2-datatable": "^0.5.3"
  }
}
